{
  "name": "solscan-agent",
  "version": "0.1.0",
  "description": "A Model Context Protocol server",
  "private": true,
  "type": "module",
  "bin": {
    "solscan-agent": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js"
  },
  "dependencies": {
    "@jup-ag/api": "^6.0.42",
    "@modelcontextprotocol/sdk": "0.6.0",
    "@solana/web3.js": "^1.98.2",
    "@types/bs58": "^4.0.4",
    "@types/figlet": "^1.7.0",
    "axios": "^1.9.0",
    "bs58": "^6.0.0",
    "cli-table3": "^0.6.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "figlet": "^1.8.1",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  }
}
